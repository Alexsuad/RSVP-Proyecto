{
  "cleanUrls": false,
  "trailingSlash": true,
  "rewrites": [
    { "source": "/admin", "destination": "/admin/login.html" },
    { "source": "/admin/", "destination": "/admin/login.html" },

    { "source": "/app/login", "destination": "/app/login.html" },
    { "source": "/app/login/", "destination": "/app/login.html" },

    { "source": "/app", "destination": "/app/request-access.html" },
    { "source": "/app/", "destination": "/app/request-access.html" },

    { "source": "/", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}

